<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.mian.ActiveFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_session"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_main_recycler"
        android:clipToPadding="false"
        android:elevation="1dp"
        android:outlineProvider="bounds"
        android:paddingBottom="@dimen/len_12"
        android:paddingTop="@dimen/len_12"
        android:visibility="gone"
        tools:targetApi="lollipop" />

    <com.wy.common.widget.EmptyView
        android:id="@+id/ev_empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</FrameLayout>